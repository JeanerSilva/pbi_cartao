{
  "compilerOptions": {
    "target": "ES2017",
    "module": "es2020",
    "outDir": ".tmp/build",
    "allowJs": true,
    "sourceMap": true,
    "declaration": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2020",
      "dom"
    ],
    "strict": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true
  },
  "files": [
    "src/visual.ts",
    "src/settings.ts",
    "node_modules/powerbi-visuals-api/index.d.ts"
  ],
  "include": [
    "node_modules/powerbi-visuals-api/src/formatting-model-api.d.ts"
  ]
}